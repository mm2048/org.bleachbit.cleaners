<cleaner id="gmrun" os="linux">
  <label>gmrun</label>
  <description>Featureful CLI-like GTK+ application launcher</description>
  <option id="history">
    <label>History</label>
    <description>Delete command history</description>
    <action search="file" command="delete" path="~/.gmrun_history"/>
  </option>
</cleaner>
