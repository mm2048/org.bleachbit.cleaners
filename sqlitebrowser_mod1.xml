<cleaner id="sqlitebrowser" os="linux">
  <label>sqlitebrowser</label>
  <description>sqlite database browser</description>
  <option id="mru">
    <label>Most recently used</label>
    <description>Delete the list of recently used files</description>
    <action command="ini" section="%General" search="file" path="~/.config/sqlitebrowser/sqlitebrowser.conf"/>
  </option>
</cleaner>
